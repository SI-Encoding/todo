type Query {
  getList: [Tasks]
}

type Mutation {
    createTask(id: ID!, name: String!, notes: String!, dueDate: String!, time: String!, isComplete: Boolean!) : Tasks!
    taskCompleted(id: ID!, isComplete: Boolean!): Tasks!
}

type Tasks {
  id: ID!
  name: String!
  notes: String!
  dueDate: String!
  time: String!
  isComplete: Boolean!
}
